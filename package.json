{
  "name": "minecraft-silver-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production apiHost=https://silverland.fun/api/v1",
    "build:dev": "webpack --env mode=development",
    "typescript:validate": "tsc --noEmit",
    "typescript:swagger": "swagger-typescript-api --path ./.api-docs --output ./src/app/types/api --name apiTypes.ts  --no-client",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "test:unit": "jest --config ./config/jest/jest.config.ts --passWithNoTests",
    "test:ui": "npx loki test",
    "test:ui:approve": "npx loki approve",
    "storybook": "STORYBOOK_IS_DEV=true storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^1.7.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-webpack5": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.0.0",
    "@types/dompurify": "^3.0.5",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "babel-jest": "^29.7.0",
    "css-loader": "^6.10.0",
    "dotenv": "^16.4.1",
    "dotenv-webpack": "^8.0.1",
    "html-webpack-plugin": "^5.6.0",
